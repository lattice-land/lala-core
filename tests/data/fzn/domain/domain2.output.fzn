% var int: x;
% var int: c_0;
% var int: z_0;
% var int: z_1; 
% var bool: b_2; 
% var int: c_10; 
% var bool: b_3;

% var int: y;
% var int: z_4;
% var int: z_5;
% var bool: b_6;
% var bool: b_7;

% constraint int_eq(c_0,0);
% constraint int_eq(c_10,10);

% % constraint for x
% constraint int_eq(z_0,int_minus(c_0,x));
% constraint int_eq(z_1,int_minus(c_0,c_0));
% constraint int_eq(b_2,int_le(z_0,z_1)); 
% constraint int_eq(b_3,int_le(x,c_10));

% % constraint for y
% constraint int_eq(z_4,int_minus(c_0,y));
% constraint int_eq(z_5,int_minus(c_0,c_0));
% constraint int_eq(b_6,int_le(z_4,z_5)); 
% constraint int_eq(b_7,int_le(y,c_10));

var int: x; 
constraint int_ge(x,0); 
constraint int_le(x,10);

var int: y; 
constraint int_ge(y,0); 
constraint int_le(y,10);